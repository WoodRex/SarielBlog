{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "2.2.2",
    "prettier-plugin-tailwindcss": "^0.1.13"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true,
    "rules": {
      "import/order": "off"
    }
  },
  "engines": {
    "node": ">=14.19 <=16.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "@uiw/react-md-editor": "^3.18.1",
    "html-truncate": "^1.2.2",
    "markdown-it": "^13.0.1",
    "rehype-sanitize": "^5.0.1"
  }
}
